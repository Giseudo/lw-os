{
  "name": "lw-os",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:storybook": "vue-cli-service build:storybook -c private/storybook && node ./build-scripts/fix-static-storybook.js",
    "serve:storybook": "vue-cli-service serve:storybook -p 8001 -c private/storybook -s public/",
    "serve:test": "vue-cli-service test:unit --watch src/**/*.spec.js"
  },
  "dependencies": {
    "bourbon-neat": "^3.0.1",
    "core-js": "^2.6.5",
    "three": "^0.93.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "zlibjs": "^0.3.1"
  },
  "devDependencies": {
    "@storybook/addon-storysource": "5.0.11",
    "@storybook/addon-actions": "4.0.0-alpha.20",
    "@storybook/addon-options": "4.0.0-alpha.20",
    "@storybook/vue": "4.0.0-alpha.20",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-service": "^3.1.1",
    "@vue/cli-plugin-unit-mocha": "^3.1.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-polyfill": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "storybook-addon-vue-info": "^0.6.1",
    "storybook-vue-router": "^1.0.1",
    "vue-cli-plugin-atomic-design": "^0.2.12",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
